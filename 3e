using System;

class Program
{
    static void Main()
    {
        Console.Write("Digite o primeiro número: ");
        float numero1 = Convert.ToSingle(Console.ReadLine());
        Console.Write("Digite o segundo número: ");
        float numero2 = Convert.ToSingle(Console.ReadLine());
        if (numero1 >= numero2)
        {
            Console.WriteLine("O primeiro número é maior ou igual ao segundo número.");
        }
        else
        {
            Console.WriteLine("O primeiro número não é maior nem igual ao segundo número.");
             Console.ReadKey();
        }
    }
}
